{
	"name": "Impetus",
	"version": "1.0",
	"description": "The best chrome extension ever. Biblical Momentum = Impetus!",
	"permissions": ["activeTab", "declarativeContent", "storage"],
	"options_page": "options.html",
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",

	"chrome_url_overrides" : {
    "newtab": "tabPage.html"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/impetus.png",
      "32": "images/impetus.png",
      "48": "images/impetus.png",
      "128": "images/impetus.png"
    }
  },
  "icons": {
    "16": "images/impetus.png",
    "32": "images/impetus.png",
    "48": "images/impetus.png",
    "128": "images/impetus.png"
  },
	"manifest_version": 2
}
